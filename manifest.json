{
  "name": "Codemarks",
  "version": "1.1",
  "description": "Save codemarks anywhere",
  "content_scripts": [
    {
      "matches": ["http://jquery.com/*"],
      "js": ["jquery-1.7.2.min.js"]
    }
  ],
  "permissions": [
     "tabs"
  ],
  "browser_action": {
    "default_icon": "cm2.png",
    "popup": "popup.html"
  }
}
